{
  "name": "@adexchange/aeg-security",
  "version": "0.0.4",
  "description": "Camp 2.0 security",
  "main": "./lib/index.js",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "git://git@bitbucket.org:AdExchangeGrp/aeg-security.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register test/",
    "test:organization": "mocha --recursive --compilers js:babel-core/register test/integration/aurora.js test/integration/organization.integration.test.js",
    "test:application": "mocha --recursive --compilers js:babel-core/register test/integration/aurora.js test/integration/application.integration.test.js",
    "test:directory": "mocha --recursive --compilers js:babel-core/register test/integration/aurora.js test/integration/directory.integration.test.js",
    "test:group": "mocha --recursive --compilers js:babel-core/register test/integration/aurora.js test/integration/group.integration.test.js",
    "test:account": "mocha --recursive --compilers js:babel-core/register test/integration/aurora.js test/integration/account.integration.test.js",
    "test:apikey": "mocha --recursive --compilers js:babel-core/register test/integration/aurora.js test/integration/api-key.integration.test.js",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers js:babel-core/register test/",
    "eslint": "eslint .",
    "eslint:watch": "npm run eslint && onchange \"**/*.js\" \".jshint*\" -- npm run eslint",
    "flow": "flow check",
    "compile": "babel -d ./lib/ ./src/ && flow-copy-source ./src ./lib",
    "build": "npm run eslint && npm run flow && npm run compile && istanbul cover _mocha -- --recursive --compilers js:babel-core/register --reporter mocha-junit-reporter test/unit",
    "patch": "git reset --hard HEAD && git clean -fd && npm run compile && npm version patch -m \"[ci skip] %s\" && git push origin master && git push origin master --tags && npm publish"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.45.0",
    "flow-copy-source": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "mocha-junit-reporter": "^1.13.0",
    "onchange": "^3.2.1",
    "should": "^11.2.1"
  },
  "dependencies": {
    "@adexchange/aeg-common": "0.0.37",
    "@adexchange/aeg-mysql": "0.0.35",
    "@adexchange/aeg-redis": "0.0.30",
    "babel-runtime": "^6.23.0",
    "bcrypt": "^1.0.2",
    "config": "^1.26.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "njwt": "^0.4.0",
    "secure-random": "^1.1.1",
    "slug": "^0.9.1",
    "uuid": "^3.0.1"
  },
  "engine": "node >= 6.10.0"
}
